    {
      "parameters": {
        "jsCode": " // Obtener datos originales del workflow\n  const originalData = $('Add Folder Info (from Tags)').item.json;\n\n  // Verificar si el archivo existe\n  const githubResponse = $input.item.json;\n  const fileExists = !!(githubResponse.sha);\n  const sha = githubResponse.sha || null;\n\n  // Crear copia limpia del workflow\n  const workflowToSave = { ...originalData };\n  delete workflowToSave._filePath;\n  delete workflowToSave._fileName;\n  delete workflowToSave._folderName;\n  delete workflowToSave.shared;\n  delete workflowToSave.activeVersion;\n  delete workflowToSave.activeVersionId;\n  delete workflowToSave.staticData;\n\n  // Convertir a base64\n  const newContent = JSON.stringify(workflowToSave, null, 2);\n  const base64Content = Buffer.from(newContent).toString('base64');\n\n  // Comparar con contenido existente en GitHub\n  let hasChanges = true;\n  if (fileExists && githubResponse.content) {\n    // Decodificar contenido de GitHub (viene en base64)\n    const existingContent = Buffer.from(githubResponse.content, 'base64').toString('utf8');\n    hasChanges = existingContent !== newContent;\n  }\n\n  return {\n    json: {\n      _filePath: originalData._filePath,\n      _fileName: originalData._fileName,\n      _fileExists: fileExists,\n      _hasChanges: hasChanges,\n      _sha: sha,\n      _content: base64Content\n    }\n  };"
      },
